@model CardiologicalResearch.Models.MedicalRecord

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>MedicalRecord</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.MedicalRecordDate)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.MedicalRecordDate)
            @Html.ValidationMessageFor(model => model.MedicalRecordDate)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.UserId, "User")
        </div>
        <div class="editor-field">
            @Html.DropDownList("UserId", String.Empty)
            @Html.ValidationMessageFor(model => model.UserId)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
